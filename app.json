{
  "expo": {
    "name": "MeditApp",
    "slug": "MeditApp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Project inspired by the design of Natalia Lebedeva available on Dribbble. Developed by me in React Native.",
    "githubUrl": "https://github.com/wanderhungerbuhler/MeditApp"
  }
}
